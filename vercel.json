{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python",
      "config": {
        "build": "pip install -r requirements.txt && playwright install --with-deps"
      }
    }
  ],
  "functions": {
    "api/webhook.py": {
      "maxDuration": 45,
      "memory": 1024,
      "includeFiles": [
        "resutbot.py",
        "results_helper.py",
        "ExamTimeTable.py"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/webhook.py"
    }
  ]
}
